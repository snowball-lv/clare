

// #define FUNC(type, name, params, body)

FUNC(int32_t, GetI32, (Node *node), {
    return node->i32;
})

FUNC(int, GetArgIndex, (Node *node), {
    return node->arg_index;
})

FUNC(const char *, GetFuncName, (Node *node), {
    return node->name;
})

FUNC(List *, GetArgs, (Node *node), {
    return node->args;
})

FUNC(void, DeleteNodeTree, (Node *root), {
    Set *set = NewSet();
    CollectNodes(set, root);
    DeleteNodeSet(set);
    DeleteSet(set);
})
