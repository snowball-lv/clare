


func main

    call(i32, ext_printf, str("\n"))
    call(i32, ext_printf, str("pizÄ£ec\n"))
    
    call(i32, ext_printf, str("\n"))
    $i = i32(0)
    while lt($i, i32(10)) do
        call(i32, ext_printf,
            str("clare_fib(%i): %i\n"),
            $i,
            call(i32, clare_fib, $i))
        $i = add($i, i32(1))
    end
    
    call(i32, ext_printf, str("\n"))
    $k = i32(0)
    while lt($k, i32(10)) do
        call(i32, ext_printf,
            str("clare_fibf(%i): %f\n"),
            $k,
            ftd(call(float, clare_fibf, itf($k))))
        $k = add($k, i32(1))
    end
        
    ret(i32(0))
end

func clare_fibf
    $n = arg(0, float)
    if eq($n, float(0)) then
        ret(float(0))
    else
        if eq($n, float(1)) then
            ret(float(1))
        else
            ret(add(
                call(float, clare_fibf, add($n, float(-1))),
                call(float, clare_fibf, add($n, float(-2)))))
        end
    end
end

func clare_fib
    $n = arg(0, i32)
    if eq($n, i32(0)) then
        ret(i32(0))
    else
        if eq($n, i32(1)) then
            ret(i32(1))
        else
            ret(add(
                call(i32, clare_fib, add($n, i32(-1))),
                call(i32, clare_fib, add($n, i32(-2)))))
        end
    end
end