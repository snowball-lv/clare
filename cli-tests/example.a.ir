

func clare_main

    $sum = i32(0)
    
    $i = i32(0)
    while lt($i, i32(10)) do
        
        if lt($i, i32(5)) then
            $sum = add($sum, $i)
        else
            $sum = add($sum, i32(1000))
        end
        
        $i = add($i, i32(1))
    end
    
    ret($sum)
end
