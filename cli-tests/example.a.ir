


func clare_fibf
    $n = arg(0, float)
    if eq($n, float(0)) then
        ret(float(0))
    else
        if eq($n, float(1)) then
            ret(float(1))
        else
            ret(add(
                float(-1),
                float(-2)))
        end
    end
end


func clare_fib
    $n = arg(0, i32)
    if eq($n, i32(0)) then
        ret(i32(0))
    else
        if eq($n, i32(1)) then
            ret(i32(1))
        else
            ret(add(
                call(i32, clare_fib, add($n, i32(-1))),
                call(i32, clare_fib, add($n, i32(-2)))))
        end
    end
end
