




func clare_fib
    $n = arg(0, i32)
    if eq($n, i32(0)) then
        ret(i32(0))
    else
        if eq($n, i32(1)) then
            ret(i32(1))
        else
            ret(add(
                call(i32, clare_fib, add($n, i32(-1))),
                call(i32, clare_fib, add($n, i32(-2)))))
        end
    end
end
